# nginx.default

server {
    listen 8029;
    server_name cactugram.org;

    location / {
        proxy_pass http://127.0.0.1:8019;
        proxy_set_header Host $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }
    location /static {
        root /opt/app/cactugram;
    }
}